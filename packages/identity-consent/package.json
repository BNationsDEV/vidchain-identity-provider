{
  "name": "vidchain-login-provider",
  "version": "0.0.1",
  "description": "Validated ID Open Identity Provider",
  "author": "Validated ID <info@validatedid.com>",
  "private": true,
  "license": "ISC",
  "scripts": {
    "audit": "audit-ci --moderate",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "coverage": "jest --coverage",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint-staged": "lint-staged",
    "lint": "npm lint:ts && npm lint:tsc && npm lint:prettier",
    "lint:ts": "eslint . --ext .js,.ts ",
    "lint:tsc": "tsc --noEmit --incremental false",
    "lint:prettier": "prettier . --check",
    "release": "standard-version",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/bull": "^0.3.1",
    "@nestjs/cli": "^7.5.4",
