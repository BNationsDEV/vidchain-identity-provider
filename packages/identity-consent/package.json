    "@validatedid/did-auth": "^2.1.2",
    "axios": "^0.21.1",
    "base-58": "0.0.1",
    "base-64": "^1.0.0",
    "base64url": "^3.0.1",
    "bull": "^3.20.0",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "did-jwt": "^4.8.1",
    "did-resolver": "^2.1.2",
    "dotenv": "^8.2.0",
    "ethers": "^5.0.24",
    "express": "^4.17.1",
    "hbs": "^4.1.1",
    "ioredis": "^4.19.4",
    "jose": "^3.5.0",
    "jwt-decode": "^3.1.2",
    "key-encoder": "^2.0.3",
    "nest-winston": "^1.4.0",
    "node-fetch": "^2.6.1",
    "qrcode": "^1.4.4",
    "querystring": "^0.2.0",
    "reflect-metadata": "^0.1.13",
